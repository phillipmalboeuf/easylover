<section>
	{% for block in section.blocks %}
  <div class="padded padded--thick text_center">
    <h2 class="">{{block.settings.title}}</h2>
    {% assign blog = blogs[block.settings.blog] %}
    {% include 'lookbook' %}
    {% endfor %}
  </div>
</section>


{% schema %}
{
  "name": "Lookbooks",
  "settings": [
  ],
  "blocks": [
    {
      "type": "blog",
      "name": "Blog",
      "settings": [
        {
          "id": "title",
          "type": "text",
          "label": "Title"
        },
        {
          "id": "blog",
          "type": "blog",
          "label": "Blog"
        }
      ]
    }
  ]
}
{% endschema %}